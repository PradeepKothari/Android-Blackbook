<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kogent.android"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.kogent.android.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UserDetails" android:label="UserDetails">
  	<intent-filter>
 		<action android:name="com.kogent.android.UserDetails" />
 		<category android:name="android.intent.category.DEFAULT" />
 	</intent-filter>
</activity>
<activity android:name=".CustomerDetails" android:label="CustomerDetails">
  	<intent-filter>
 		<action android:name="com.kogent.android.UserDetails" />
 		<category android:name="android.intent.category.DEFAULT" />
 	</intent-filter>
</activity>
        
    </application>

</manifest>
